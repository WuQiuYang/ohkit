{
  "name": "@ohkit/prefix-classname",
  "version": "0.0.3-alpha.0",
  "description": "add prefix for classnames",
  "keywords": [
    "prefix-classname"
  ],
  "homepage": "",
  "license": "ISC",
  "source": "src/index.@(js|jsx|mjs|ts|tsx)",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WuQiuYang/ohkit"
  },
  "scripts": {
    "build": "npm run clean && microbundle --jsx React.createElement",
    "dev": "npm run watch",
    "watch": "microbundle watch --jsx React.createElement --no-compress --format es --output dist/index.es.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  }
}
