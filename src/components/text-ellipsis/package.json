{
  "name": "@ohkit/text-ellipsis",
  "version": "0.0.3-alpha.1",
  "description": "text ellipsis for react",
  "keywords": [
    "text ellipsis"
  ],
  "author": "wuqiuyang <wuqiuyang305@126.com>",
  "homepage": "",
  "license": "ISC",
  "source": "src/index.@(js|jsx|mjs|ts|tsx)",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WuQiuYang/ohkit"
  },
  "scripts": {
    "build": "npm run clean && microbundle --jsx React.createElement",
    "dev": "npm run watch",
    "watch": "microbundle watch --jsx React.createElement --no-compress --format es --output dist/index.es.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ohkit/measure": "0.0.3-alpha.0",
    "@ohkit/utils": "0.0.3-alpha.1"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "gitHead": "3d62d95c06d9a8e0e52d68c924960e3b48d82238"
}
